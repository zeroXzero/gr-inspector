<block>
  <name>QT GUI Inspector Sink</name>
  <key>inspector_qtgui_sink_vf</key>
  <category>Inspector</category>
  <import>import inspector</import>
  <make>#set $win = 'self._%s_win'%$id
    inspector.qtgui_inspector_sink_vf($fft_len)</make>
  <param>
    <name>Fft_len</name>
    <key>fft_len</key>
    <type>int</type>
  </param>
  <sink>
    <name>in</name>
    <type>float</type>
    <vlen>$fft_len</vlen>
  </sink>
  <sink>
    <name>map_in</name>
    <type>message</type>
  </sink>
</block>
